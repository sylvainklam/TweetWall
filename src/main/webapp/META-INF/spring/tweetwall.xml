<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="org.jahia.modules.tweetwall.actions.StartTweetWallServer" >
        <property name="name" value="startTWS"/>
        <property name="requireAuthenticatedUser" value="false"/>
    </bean>
    <bean class="org.jahia.modules.tweetwall.actions.StopTweetWallServer" >
        <property name="name" value="stopTWS"/>
        <property name="requireAuthenticatedUser" value="false"/>
    </bean>
</beans>